<h1>Users#my_product</h1>
<% @user.products.each do |product| %>
  <div>
    <%= product.title %>
    <%= product.text %>
    <%= product.status %>
    <% if product.user_id == current_user.id %>
      <%= link_to "編集する", edit_product_path(product) %>
      <%= link_to "削除する", product_path(product), method: :delete, data: {confirm: "本当に削除しますか？"} %>
    <% end %>
  </div>
<% end %>